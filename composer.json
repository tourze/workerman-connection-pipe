  "license": "MIT",
  "require": {
    "php": "^8.1",
    "psr/log": "^3|^2|^1",
    "symfony/event-dispatcher": "^6.4",
    "symfony/event-dispatcher-contracts": "^2.5 | ^3",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
