{
  "name": "tourze/workerman-connection-pipe",
  "description": "连接间转发，支持TCP和UDP",
  "license": "MIT",
  "require": {
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "tourze/enum-extra": "0.1.*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\ConnectionPipe\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\ConnectionPipe\\Tests\\": "tests"
    }
